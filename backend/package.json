{
  "name": "ecotrace-backend",
  "version": "1.0.0",
  "description": "EcoTrace real-time carbon footprint tracking backend with Appwrite",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy": "appwrite deploy",
    "setup-db": "tsx scripts/setup-database.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration --runInBand",
    "test:e2e": "jest --testPathPattern=tests/e2e --runInBand",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "migrate": "tsx src/shared/database/migration-manager.ts",
    "migrate:rollback": "tsx scripts/rollback-migrations.ts",
    "migrate:status": "tsx scripts/migration-status.ts",
    "precommit": "npm run lint && npm run format:check && npm run test:ci",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.3.16",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "node-appwrite": "^13.0.0",
    "node-cron": "^4.2.1",
    "opossum": "^9.0.0",
    "papaparse": "^5.5.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.0",
    "socket.io": "^4.8.1",
    "supertest": "^7.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.0",
    "@types/opossum": "^8.1.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "appwrite-cli": "^4.1.0",
    "eslint": "^9.35.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "carbon-tracking",
    "appwrite",
    "real-time",
    "sustainability"
  ],
  "author": "EcoTrace Team",
  "license": "MIT"
}
