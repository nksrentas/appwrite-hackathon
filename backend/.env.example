# Appwrite Configuration
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=ecotrace-dashboard
APPWRITE_API_KEY=your_api_key_here
APPWRITE_DATABASE_ID=ecotrace-main

# External API Keys
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_WEBHOOK_SECRET=your_webhook_secret

EPA_EGRID_API_KEY=your_epa_api_key
AWS_CARBON_API_KEY=your_aws_carbon_api_key
ELECTRICITY_MAPS_API_KEY=your_electricity_maps_api_key

# Redis Cache (optional)
REDIS_URL=redis://localhost:6379

# Environment
NODE_ENV=development
PORT=3000

# Security
JWT_SECRET=your_jwt_secret_here
WEBHOOK_SECRET=your_webhook_secret_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100